#include<iostream>
using namespace std;

int main()
{
    int num1,num2;
    cout<<"enter the value of num 1 \n";//<< is called insertion operator
    cin>>num1;// cin>> is to take the value form user
    cout<<"enter the value of num 2 \n";//<< is called insertion operator
    cin>>num2;//>> is called extraction operator
    cout<<"The sum of num1 and num2 is "<< num1+num2;
    return 0;
}
